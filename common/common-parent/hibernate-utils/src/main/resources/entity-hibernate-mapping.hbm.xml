<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.techmania.entity.Airport" table="AIRPORT">
		<id name="airportCode" column="AIRPORT_CODE">
		</id>
		<property name="airportDescription" type="string" column="AIRPORT_DESCRIPTION" />
		<property name="active" column="ACTIVE" />
	</class>
</hibernate-mapping>